{
  "name": "vocabvault",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "^0.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "turbo": "^2.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@10.8.1",
  "dependencies": {
    "tailwindcss-radix": "^3.0.3"
  }
}
